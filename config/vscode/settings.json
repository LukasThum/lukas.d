{
  // the themes

  "workbench.colorTheme": "Noctis Lux",
  "window.autoDetectColorScheme": false,
  "workbench.colorThemeLight": "Noctis Lux",
  "workbench.colorThemeDark": "Catppuccin Mocha",
  "workbench.preferredLightColorTheme": "Noctis Lux",
  "workbench.preferredDarkColorTheme": "Catppuccin Mocha",

  // it's all about the fonts
  "editor.fontSize": 14,
  "editor.letterSpacing": -0.5,
  "editor.lineHeight": 1.1,
  "workbench.fontAliasing": "auto",

  "terminal.integrated.fontSize": 12,
  "terminal.integrated.letterSpacing": -0.5,
  // "terminal.integrated.lineHeight": 1,

  "editor.fontFamily": "Cartograph CF", "editor.fontLigatures": "'ss02'", "editor.fontWeight": "350",
  // "editor.fontFamily": "1raCode Nerd Font", "editor.fontLigatures": true,
  // "editor.fontFamily": "JetBrainsMono Nerd Font", "editor.fontLigatures": true, "editor.fontWeight": "350",
  // "editor.fontFamily": "Cascadia Code", "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",

  "editor.comments.insertSpace": false,
  "editor.detectIndentation": false,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80],
  "editor.tabSize": 2,
  "editor.useTabStops": false,
  "files.trimTrailingWhitespace": true,
  "workbench.editor.closeEmptyGroups": true,
  "workbench.startupEditor": "none",

  // random stuff
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.defaultProfile.osx": "fish",
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 80,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },

  // hello mssql
  "mssql.connections": [
    {
      "server": "localhost",
      "database": "",
      "authenticationType": "SqlLogin",
      "user": "sa",
      "password": "",
      "emptyPasswordInput": false,
      "savePassword": true,
      "profileName": "localhost",
      "encrypt": "Mandatory",
      "trustServerCertificate": true
    }
  ],

  // whatever
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },


  /* ai
  "aws.suppressPrompts": {
    "codeWhispererConnectionExpired": true
  },
  "github.copilot.editor.enableAutoCompletions": false,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "kotlin": false
  },
  */

  // shut up..
  "redhat.telemetry.enabled": false,
  "audioCues.diffLineDeleted": "off",
  "audioCues.diffLineInserted": "off",
  "audioCues.diffLineModified": "off",
  "audioCues.lineHasBreakpoint": "off",
  "audioCues.lineHasError": "off",
  "audioCues.lineHasFoldedArea": "off",
  "audioCues.lineHasInlineSuggestion": "off",
  "audioCues.noInlayHints": "off",
  "audioCues.notebookCellCompleted": "off",
  "audioCues.notebookCellFailed": "off",
  "audioCues.onDebugBreak": "off",
  "audioCues.taskCompleted": "off",
  "audioCues.taskFailed": "off",
  "audioCues.terminalCommandFailed": "off",
  "audioCues.terminalQuickFix": "off",
  "css.completion.completePropertyWithSemicolon": false,
  "css.completion.triggerPropertyValueCompletion": false,
  "debug.console.historySuggestions": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingDelete": "never",
  "editor.autoClosingOvertype": "never",
  "editor.autoClosingQuotes": "never",
  "editor.autoIndent": "keep",
  "editor.autoSurround": "never",
  "editor.inlineSuggest.enabled": false,
  "editor.quickSuggestions": { "other": "off" },
  "editor.quickSuggestionsDelay": 99999,
  "editor.snippetSuggestions": "none",
  "editor.suggest.filterGraceful": false,
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.suggest.selectionMode": "never",
  "editor.suggest.showClasses": false,
  "editor.suggest.showColors": false,
  "editor.suggest.showConstants": false,
  "editor.suggest.showConstructors": false,
  "editor.suggest.showCustomcolors": false,
  "editor.suggest.showDeprecated": false,
  "editor.suggest.showEnumMembers": false,
  "editor.suggest.showEnums": false,
  "editor.suggest.showEvents": false,
  "editor.suggest.showFields": false,
  "editor.suggest.showFiles": false,
  "editor.suggest.showFolders": false,
  "editor.suggest.showFunctions": false,
  "editor.suggest.showIcons": false,
  "editor.suggest.showInlineDetails": false,
  "editor.suggest.showInterfaces": false,
  "editor.suggest.showIssues": false,
  "editor.suggest.showKeywords": false,
  "editor.suggest.showMethods": false,
  "editor.suggest.showModules": false,
  "editor.suggest.showOperators": false,
  "editor.suggest.showProperties": false,
  "editor.suggest.showReferences": false,
  "editor.suggest.showSnippets": false,
  "editor.suggest.showStructs": false,
  "editor.suggest.showTypeParameters": false,
  "editor.suggest.showUnits": false,
  "editor.suggest.showUsers": false,
  "editor.suggest.showValues": false,
  "editor.suggest.showVariables": false,
  "editor.suggest.showWords": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.wordBasedSuggestions": false,
  "elixirLS.autoInsertRequiredAlias": false,
  "elixirLS.signatureAfterComplete": false,
  "elixirLS.suggestSpecs": false,
  "emmet.showAbbreviationSuggestions": false,
  "explorer.fileNesting.expand": false,
  "extensions.ignoreRecommendations": true,
  "git.suggestSmartCommit": false,
  "html.autoClosingTags": false,
  "html.autoCreateQuotes": false,
  "html.completion.attributeDefaultValue": "empty",
  "html.suggest.html5": false,
  "interactiveWindow.alwaysScrollOnNewCell": false,
  "javascript.autoClosingTags": false,
  "javascript.format.insertSpaceAfterCommaDelimiter": false,
  "javascript.preferences.jsxAttributeCompletionStyle": "none",
  "javascript.suggest.autoImports": false,
  "javascript.suggest.classMemberSnippets.enabled": false,
  "javascript.suggest.completeJSDocs": false,
  "javascript.suggest.enabled": false,
  "javascript.suggest.includeAutomaticOptionalChainCompletions": false,
  "javascript.suggest.includeCompletionsForImportStatements": false,
  "javascript.suggest.jsdoc.generateReturns": false,
  "javascript.suggest.names": false,
  "javascript.suggest.paths": false,
  "javascript.suggestionActions.enabled": false,
  "less.completion.completePropertyWithSemicolon": false,
  "less.completion.triggerPropertyValueCompletion": false,
  "markdown.suggest.paths.enabled": false,
  "npm.fetchOnlinePackageInfo": false,
  "output.smartScroll.enabled": false,
  "php.suggest.basic": false,
  "scss.completion.triggerPropertyValueCompletion": false,
  "terminal.integrated.detectLocale": "off",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "typescript.autoClosingTags": false,
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.preferences.jsxAttributeCompletionStyle": "none",
  "typescript.suggest.autoImports": false,
  "typescript.suggest.classMemberSnippets.enabled": false,
  "typescript.suggest.completeJSDocs": false,
  "typescript.suggest.enabled": false,
  "typescript.suggest.includeAutomaticOptionalChainCompletions": false,
  "typescript.suggest.includeCompletionsForImportStatements": false,
  "typescript.suggest.jsdoc.generateReturns": false,
  "typescript.suggest.objectLiteralMethodSnippets.enabled": false,
  "typescript.suggest.paths": false,
  "typescript.suggestionActions.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.list.typeNavigationMode": "trigger"
}